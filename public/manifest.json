{
  "short_name": "redfin-augmenter",
  "name": "redfin-augmenter",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
   "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.redfin.com/*"],
      "js": ["content-script.js"]
